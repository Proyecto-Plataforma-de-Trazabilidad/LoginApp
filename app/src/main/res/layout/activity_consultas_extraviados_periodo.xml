<?xml version="1.0" encoding="utf-8"?>
<ScrollView
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical"
android:background="@color/white">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Indexs.Movimientos.Productores.Extraviados.ConsultasExtraviadosPeriodo">

    <LinearLayout
        android:id="@+id/layoutbase"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:padding="20dp"
        android:background="#FFFFFF">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="5dp">
            <ImageView
                android:id="@+id/apejal"
                android:layout_width="70dp"
                android:layout_height="70dp"
                app:srcCompat="@drawable/apeajal2" />
            <ImageView
                android:id="@+id/asica"
                android:layout_width="50dp"
                android:layout_height="70dp"
                android:layout_alignLeft="@+id/apejal"
                android:layout_alignParentRight="true"
                android:layout_gravity="top"
                android:layout_marginLeft="15dp"
                app:srcCompat="@drawable/asica" />
            <ImageView
                android:id="@+id/Amocali"
                android:layout_width="60dp"
                android:layout_height="70dp"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="15dp"
                app:srcCompat="@drawable/fidaixbm_400x400" />

        </LinearLayout>
        <TextView
            android:id="@+id/tit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Consulta Por Periodo De Extraviados "
            android:textStyle="bold"
            android:padding="20dp"
            android:textAlignment="center"
            android:textColor="@color/black" />
        <TextView
            android:id="@+id/f1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fecha inicial:"
            android:textColor="@color/black"
            android:padding="10dp"
            android:textSize="18dp"
            app:layout_constraintTop_toBottomOf="@+id/tit" />
        <EditText
            android:id="@+id/FI"
            android:hint="año/mes/día"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:clickable="true"
            android:focusable="false"
            android:maxLines="1"
            android:layout_margin="10dp"/>

        <TextView
            android:id="@+id/f2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Fecha Final:"
            android:textColor="@color/black"
            android:padding="10dp"
            android:textSize="18dp"
            app:layout_constraintTop_toBottomOf="@+id/tit" />
        <EditText
            android:id="@+id/FF"
            android:hint="año/mes/día"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:clickable="true"
            android:focusable="false"
            android:maxLines="1"
            android:layout_margin="10dp"/>

        <TableLayout
            android:paddingTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titn"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/encabezado1"
                    android:text="dato1"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/titd"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/encabezado1"
                    android:text="Dato2"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/titla"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/encabezado1"
                    android:text="Dato3"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/titlo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/encabezado1"
                    android:text="  Dato4 "
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

            </TableRow>
        </TableLayout>
        <TableLayout
            android:id="@+id/tablaO"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="-5dp">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </TableLayout>


        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="16dp">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/btnreg1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:text="@string/Regresar"
                android:layout_column="0"
                android:layout_row="1"
                android:backgroundTint="@color/Verdeapejal"
                android:textColor="@color/white"
                android:layout_centerHorizontal="true"
                />


        </GridLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>